<%= link_to page, id: dom_id(page), class: "py-4 hover:bg-gray-100" do %>
  <div class="flex justify-between">
    <div>
      <h5 class="font-bold"><%= page.name %></h5>
      <div class="text-sm text-gray-700"><%= page.url %></div> 
    </div>

    <%= render page.last_result if page.results.last.present? %>
  </div>
<% end %>
